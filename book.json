{
  "title": "Flynt Documentation",
  "structure": {
    "summary": "table-of-contents.md"
  },
  "gitbook": "3.1.1",
  "plugins": ["prism", "-highlight", "copy-code-button", "word-count", "collapsible-menu", "custom-favicon", "components", "edit-link", "-fontsettings", "nestedmd", "styles-stylus"],
  "styles": {
    "website": "styles/website.styl"
  },
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "components": {
      "templatePath": "templates",
      "templates": [
        {
          "name": "header",
          "target": "body"
        }
      ]
    },
    "favicon": "assets/favicon.ico",
    "edit-link": {
      "base": "https://github.com/bleech/flynt-docs/edit/master",
      "label": "Edit on Github"
    }
  }
}
